public class DequeTest
{
    public static void main(String[] args) {

        Deque<String> dll = new Deque();


        dll.Insert_at_the_end("Arkan");


        dll.Insert_at_the_beginning("Messi");


        dll.Insert_at_the_beginning("Aron");

        dll.Insert_at_the_end("Kante");
        dll.Insert_at_the_end("Morni");
        dll.InsertBefore(dll.head.next, "Abel");

        dll.InsertAfter(dll.head.next, "jhon");
        dll.Remove_at_the_beginning();
        dll.Remove_at_the_end();

        dll.print_All_Node(dll.head);


    }
}
